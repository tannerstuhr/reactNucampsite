{
    "trailingComma": "es5",
    "trailingCommaLineBreak": "none",
    "endOfLine": "lf",
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "quoteProps": "as-needed",
    "arrowParens": "avoid",
    "printWidth": 100,
    "proseWrap": "always",
    "bracketSpacing": false,
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false,
    "parser": "babel"
}